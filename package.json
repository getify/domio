{
	"name": "domio",
	"description": "DOM and Event helpers for Monio",
	"version": "0.14.2",
	"main": "./src/index.js",
	"scripts": {
		"build": "npm run build-all",
		"prepare": "npm run build",
		"build-all": "node scripts/build-all.js"
	},
	"dependencies": {
		"monio": ">=0.33.0"
	},
	"devDependencies": {
		"import-remap": "~0.4.0",
		"moduloze": "~0.8.0",
		"recursive-readdir-sync": "~1.0.6"
	},
	"mz-dependencies": {
		"index.js": "DOMIO",
		"fp-helpers.js": "FPHelpers",
		"misc-helpers.js": "MiscHelpers",
		"dom-helpers.js": "DOMHelpers",
		"event-helpers.js": "EventHelpers",
		":::monio/io": "IO",
		":::monio/either": "Either",
		":::monio/io/event-stream": "IOEventStream",
		":::monio/io/helpers": "IOHelpers",
		":::monio/util": "MonioUtil"
	},
	"exports": {
		".": {
			"import": "./dist/esm/index.mjs",
			"default": "./src/index.js"
		},
		"./fp-helpers": {
			"import": "./dist/esm/fp-helpers.mjs",
			"default": "./src/fp-helpers.js"
		},
		"./misc-helpers": {
			"import": "./dist/esm/misc-helpers.mjs",
			"default": "./src/misc-helpers.js"
		},
		"./dom-helpers": {
			"import": "./dist/esm/dom-helpers.mjs",
			"default": "./src/dom-helpers.js"
		},
		"./event-helpers.js": {
			"import": "./dist/esm/event-helpers.mjs",
			"default": "./src/event-helpers.js"
		},
		"./esm": "./dist/esm/index.mjs",
		"./esm/fp-helpers": "./dist/esm/fp-helpers.mjs",
		"./esm/misc-helpers": "./dist/esm/misc-helpers.mjs",
		"./esm/dom-helpers": "./dist/esm/dom-helpers.mjs",
		"./esm/event-helpers": "./dist/esm/event-helpers.mjs",
		"./esm-browser": "./dist/esm-browser/index.mjs",
		"./esm-browser/fp-helpers": "./dist/esm-browser/fp-helpers.mjs",
		"./esm-browser/misc-helpers": "./dist/esm-browser/misc-helpers.mjs",
		"./esm-browser/dom-helpers": "./dist/esm-browser/dom-helpers.mjs",
		"./esm-browser/event-helpers": "./dist/esm-browser/event-helpers.mjs"
	},
	"repository": "getify/domio",
	"keywords": [
		"monio",
		"fp",
		"monad",
		"io",
		"DOM",
		"events"
	],
	"bugs": {
		"url": "https://github.com/getify/domio/issues",
		"email": "getify@gmail.com"
	},
	"homepage": "https://github.com/getify/domio",
	"author": "Kyle Simpson <getify@gmail.com>",
	"license": "MIT"
}
